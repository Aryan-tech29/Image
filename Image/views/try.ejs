<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
  <script src="https://cdn.ckeditor.com/4.9.1/standard/ckeditor.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <title>Document</title>
</head>
<body>
    <div class="container" >
        <div class="row">
      <% if (success != '') { %>
      <div class="alert alert-sucess">
          <%= success %>
      </div>
      <% } %>
       


    <div style=" border:1px solid blue;
    width:500px;
    height:1500px;
    padding:15px;
    position:relative;
  ">
    <div>
        <img src="https://i.pinimg.com/originals/1c/d8/3a/1cd83a72a34f6932aeb05aed12e79076.jpg" style="width: 400px; height: 400px; z-index: -10; position: inherit; display: inline-block; margin-top: 450px;" alt="">
     
    </div>
    <div class="panel-body" >
                                <form action="/try" id="EmployeeForm" class="form-horizontal" method="post"  role="form" enctype="multipart/form-data">
                                   
                                    <div class="container">
                                        <div class="row">
                                            <% if(records.length > 0) { %>
                                                <% records.forEach(function(row) { %>)
                                    <div class="image" style="width: 200px; height: 200px; position: inherit; margin-right:46px;">
                                        <img src="/uploads/<%= row.imagename %>"  style="width: 100%; height: 100%; z-index: 10;  position: relative; margin-top: -270px; margin-left: 120px; "  alt="">
                                    </div>
                                    <% } ) } %>
                                </div>
                                
                                </div>
                                    <div class="form-group">
                                        <label for="file" class="col-md-3 control-label">Upload File</label>
                                        <div class="col-md-9">
                                            <input type="file" class="form-control" name="file" required>
                                        </div>
                                    </div>
                                    
            
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="Upload" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                               
                                    
                                    
                                </form>
                             </div>
    
</div>
</div>
</div>
</body>
</body>
</html>